{
  "manifest_version": 3,
  "name": "ClipNoteJoke",
  "version": "1.0.2",
  "description": "Quick notes + random jokes from your GitHub JSON.",
  "action": {
    "default_title": "ClipNoteJoke",
    "default_popup": "popup.html"
  },
  "permissions": ["storage"],
  "host_permissions": ["https://raw.githubusercontent.com/*"],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
